{
  "name": "debian-image-builder",
  "version": "1.1.0",
  "description": "build Debian disk images (with Docker)",
  "bin": "./build",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "example": "npm run example:simple",
    "example:simple": "./build --docker --input examples/simple --output output",
    "example:uefi": "./build --docker --input examples/uefi --output output",
    "example:arm": "./build --docker --input examples/arm --output output"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ahdinosaur/debian-image-builder.git"
  },
  "keywords": [
    "debian",
    "vmdb2",
    "vmdebootrap"
  ],
  "author": "ahdinosaur",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/debian-image-builder/issues"
  },
  "homepage": "https://github.com/ahdinosaur/debian-image-builder#readme"
}
